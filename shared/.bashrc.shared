# Aliases
alias cd="z"
alias grep="rg --color=auto"
alias ls="eza --color"
alias cat="bat"
alias dl-void="cd ~/.dotfiles && stow shared && stow void && cd -"
alias dl-fedora="cd ~/.dotfiles && stow shared && stow fedora && cd -"

export EDITOR="/usr/bin/hx"

eval "$(starship init bash)"
eval "$(zoxide init bash)"
. "$HOME/.cargo/env"
